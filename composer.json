{
    "name": "cmnworks/cakestrap",
    "description": "Twitter Bootstrap plugin for CakePHP 3+",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cakestrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cakestrap\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
